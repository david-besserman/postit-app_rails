<%= render '/shared/content_title', title: 'New category' %>

<%= render '/shared/error', obj: @category %>

<%= form_for @category do |f| %>
  <div class='control-group'>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <%= f.submit 'Create category', class: "btn btn-success" %>
<% end %>
